{
  "name": "vite-x",
  "description": "A simple, DX-focused Vite + ReactJS + TailwindCSS template to hit the ground running.",
  "keywords": [
    "react",
    "reactjs",
    "vite",
    "vitejs",
    "typescript",
    "pnpm",
    "template",
    "boilerplate",
    "tailwindcss",
    "prettier",
    "eslint",
    "husky",
    "lint-staged",
    "react-testing-library",
    "vitest",
    "dark-mode"
  ],
  "homepage": "https://github.com/bkchu/vite-react-typescript-tailwindcss",
  "private": true,
  "version": "0.0.1",
  "author": {
    "name": "Brandon Chung",
    "email": "brandon@bkchu.dev",
    "url": "https://github.com/bkchu"
  },
  "scripts": {
    "dev": "vite --port 8080",
    "build": "tsc && vite build",
    "preview": "pnpm build && vite preview",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^0.7.1",
    "@floating-ui/react-dom-interactions": "^0.6.3",
    "@headlessui/react": "^1.6.1",
    "@uiw/react-md-editor": "^3.13.0",
    "axios": "^0.27.2",
    "clsx": "^1.1.1",
    "flowbite": "^1.4.7",
    "lodash.groupby": "^4.6.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.31.2",
    "react-markdown": "^8.0.3",
    "react-query": "^3.39.0",
    "react-router-dom": "^6.3.0",
    "rehype-sanitize": "^5.0.1",
    "string-strip-html": "^9.1.12"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.43",
    "@supabase/supabase-js": "^1.35.3",
    "@svgr/core": "^6.2.1",
    "@tailwindcss/typography": "^0.5.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/lodash.groupby": "^4.6.7",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.2",
    "eslint": "^8.15.0",
    "eslint-config-kentcdodds": "^20.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-testing-library": "^5.5.0",
    "husky": "^7.0.4",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.4.1",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.10",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "unplugin-icons": "^0.14.3",
    "vite": "^2.9.7",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "vitest related --run --coverage=false",
      "eslint --cache --fix",
      "prettier --write"
    ],
    "*.{css,scss,md,mdx,json}": "prettier --write"
  }
}
